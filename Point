public class Point
{
    int x;
    int y;
    
    Point(){}
    
    Point(int x, int y)
    {
        this.x=x;
        this.y=y;
    }
    
    public int hashCode()
    {
        return (x*31+y);
    }
    
    public boolean equals(Object o) {
        Point that = (Point) o;
        return x == that.x && y == that.y;
    }
}
